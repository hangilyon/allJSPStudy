	<%
		String s = request.getParameter("n");
		out.print(s);
		String[] str = request.getParameterValues("n");
		out.print("<hr>");
		for(int i=0;i<str.length;i++){
			out.print(str[i]+"<br>");
		}
		for(String n : str){
			out.print(n+"<br>"
		}
	%>
</body>
